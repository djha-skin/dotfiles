if [ -f ~/.profile -a -z "${PROFILE_INCLUDED:-}" ]
then
    source ~/.profile
fi
source ~/.zsh_aliases

if [ -e "${WORKING_LOCATION_FILE}" ]
then
    pchanged
fi
unsetopt BEEP
