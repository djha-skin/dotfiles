alias boxdown="rclone copy Dropbox:/ ${HOME}/Dropbox/"
alias boxup="rclone copy ${HOME}/Dropbox/ Dropbox:/"
alias boxsync="rclone bisync ${HOME}/Dropbox/ Dropbox:/"
alias drivedown="rclone copy HomeGoogleDrive:/ ${HOME}/HomeGoogleDrive/"
alias driveup="rclone copy ${HOME}/HomeGoogleDrive/ HomeGoogleDrive:/"
alias drivesync="rclone bisync ${HOME}/HomeGoogleDrive/ HomeGoogleDrive:/"
alias gmail="himalaya -a gmail"
alias homecal="gcalcli --config-folder=~/.config/gcalcli/home --default-calendar=djhaskin987@gmail.com"
alias pom="timer Blast 24 Pacer 1 Break 5"
alias pombreak='timer "Long Break" 30'
alias scr="screen -dRR -S ergo -t djh987"
alias skin="himalaya -a skin"
alias vim="nvim"
alias work="himalaya -a work"
alias worknotes='cd ~/Dropbox/Markdown/Work'
alias homenotes='cd ~/Dropbox/Markdown/Home'
