alias chmod *chmod $*
alias git git --no-pager $*
alias config git --git-dir=$HOME/.cfg/ --work-tree=$HOME --no-pager $*
alias sudo eshell/sudo $*
alias File $*
alias nil $*
alias find *find $*
alias add $*
alias keycode $*
alias worknotes cd ~/OneDrive/Markdown/Work
alias homenotes cd ~/OneDrive/Markdown/Home
alias diary nvim Planning/Diary/$(date +%Y-%m-%d-%A.md)
alias pom timer Blast 24 Pacer 1 Break 5
