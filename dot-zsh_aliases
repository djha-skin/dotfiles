alias vim=nvim
alias scr='screen -dRR -S ergo -t skin'
alias driveup='rclone copy /home/skin/HomeGoogleDrive/ HomeGoogleDrive:/'
alias drivedown='rclone copy HomeGoogleDrive:/ /home/skin/HomeGoogleDrive/'
